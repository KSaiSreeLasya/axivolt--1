# EmailJS Configuration
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
VITE_EMAILJS_CONTACT_TEMPLATE_ID=contact_template
VITE_EMAILJS_QUOTE_TEMPLATE_ID=quote_template
VITE_EMAILJS_JOB_TEMPLATE_ID=job_template

# Admin Email (for receiving form submissions)
VITE_ADMIN_EMAIL=contac@axivolt.in

# Supabase Configuration
VITE_PUBLIC_SUPABASE_URL=your_supabase_url_here
VITE_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key (Backend only - DO NOT expose to frontend)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# JWT Secret for session tokens
JWT_SECRET=your_secret_key_here_at_least_32_characters

# Server ping message
PING_MESSAGE=pong

# Admin Emails (comma-separated list of emails that can access admin dashboard)
ADMIN_EMAILS=admin@example.com,admin2@example.com
